<body>

	<div class="ecole"> </div>

	<div class= title>MY GRADE</div>

	<table>
		<tr class="top">
			<td><a href="./name">Firstname <img class="fleche" src="/static/grade_pict/fleche.png"></a></td>
			<td><a href="./lastname">Lastname <img class="fleche" src="/static/grade_pict/fleche.png"></a></td>
			<td><a href="./math">Mathematiques grade <img class="fleche" src="/static/grade_pict/fleche.png"></a></td>
			<td><a href="./french">French grade <img class="fleche" src="/static/grade_pict/fleche.png"></a></td>
			<td><a href="./history">History grade <img class="fleche" src="/static/grade_pict/fleche.png"></a></td>
		</tr>
	<% for (var g of grades)	{%>
		<tr>
		<td><%= g.student_first_name %></td>
		<td><%= g.student_last_name %></td>
        <td><%= g.grade_math %></td>
        <td><%= g.grade_french %></td>
		<td><%= g.grade_history %></td>
		</tr>	
	<% } %>
	</table>

<ul>
