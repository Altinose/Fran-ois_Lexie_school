<header>
	<nav>
        <label class="logo"><a href="/menu">Francois Lexis</a></label>
        <ul>
            <li><a href="/menu">Menu</a></li>
            <li><a href="/event">Event</a></li>
            <li><a href="/grades/<%=User[0]%>/">Grade</a></li>
